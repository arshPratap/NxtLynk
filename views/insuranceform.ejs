<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title></title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
    </head>
    <body>
        <h1>Insurance Form</h1>
        <form action="/addinsurance" method="POST">
        <label for="name">Name</label>
        <input type="text" name="name" id="name" value="<%=farmerName%>" readonly="true"><br>
        <label for="phone">Phone</label>
        <input type="text" name="phone" id="phone" value="<%=farmerPhone%>" readonly="true"><br>
        <label for="insurance">Choose your insurance company</label>
        <input list="insurances" name="insurance" id="insurance"><br>
        <datalist id="insurances">
            <% insList.forEach(ins=>{ %>
                <option value="<%=ins.name%>">
            <% }) %>
        </datalist>
        <button type="submit">Submit</button>
        </form>
    </body>
</html>