        <script>
            
            let stars = document.getElementById('stars');
            let moon = document.getElementById('moon');
            let sun = document.getElementById('sun');
            let animals = document.getElementById('animals');

            let mountains_behind = document.getElementById('backmountains');
            let text = document.getElementById('text');
            let btn = document.getElementById('btn');
            let mountains_front = document.getElementById('front');
            let header = document.querySelector('header');

            window.addEventListener('scroll',function(){
                let value = window.scrollY;
                if(document.getElementById('stars')){
                    document.getElementById('stars').style.left = value*0.25 + 'px';
                }
                if(document.getElementById('moon')){
                    document.getElementById('moon').style.top = value*1.05+'px';
                }
                if(document.getElementById('sun')){
                    document.getElementById('sun').style.top = value*1.05+'px';
                }
                if(document.getElementById('animals')){
                    document.getElementById('animals').style.left = value*0.25 + 'px';
                }
                mountains_behind.style.top = value*0.5+'px';
                mountains_front.style.top = value*0 + 'px';
                text.style.marginRight = value*4 + 'px';
                text.style.marginTop = value*1.5 + 'px';
                btn.style.marginTop = value*1.5 + 'px';
                header.style.top = value*0.5 + 'px';
            })
        </script>
